  "components": {
    "schemas": {
      "DocumentTypeEnum": {
        "enum": [
          "ARCHIVATION",
          "CADES",
          "PADES",
          "XADES",
          "ASIC_S",
          "ASIC_E",
          "CADES_ATTACHED",
          "CADES_DETACHED",
          "DOCX",
          "XLSX",
          "EML",
          "MSG"
        ],
        "type": "string"
      },
      "DocumentVerificationTypeEnum": {
        "enum": [
          "ADES",
          "DOCX",
          "XLSX",
          "EML",
          "MSG"
        ],
        "type": "string"
      },
      "HashAlgorithmEnum": {
        "enum": [
          "MD5",
          "MD5Cng",
          "RIPEMD160",
          "SHA1",
          "SHA1Cng",
          "SHA256",
          "SHA256Cng",
          "SHA384",
          "SHA384Cng",
          "SHA512",
          "SHA512Cng"
        ],
        "type": "string"
      },
      "ReportFormatEnum": {
        "enum": [
          "PDF",
          "XML"
        ],
        "type": "string"
      },
      "ReportTypeEnum": {
        "enum": [
          "Simple",
          "Detailed"
        ],
        "type": "string"
      },
      "RequesAuthenticationModelDto": {
        "type": "object",
        "properties": {
          "userLogin": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RequesSignedCertificate": {
        "type": "object",
        "properties": {
          "authentication": {
            "$ref": "#/components/schemas/RequesAuthenticationModelDto"
          },
          "owner": {
            "$ref": "#/components/schemas/RequesSignedCertificateModelDto"
          }
        },
        "additionalProperties": false
      },
      "RequesSignedCertificateModelDto": {
        "type": "object",
        "properties": {
          "storageId": {
            "type": "string",
            "format": "uuid"
          },
          "id": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RequesSpecificReport": {
        "type": "object",
        "properties": {
          "authentication": {
            "$ref": "#/components/schemas/RequesAuthenticationModelDto"
          },
          "report": {
            "$ref": "#/components/schemas/RequesSpecificReportModelDto"
          }
        },
        "additionalProperties": false
      },
      "RequesSpecificReportModelDto": {
        "type": "object",
        "properties": {
          "reportId": {
            "type": "string",
            "format": "uuid"
          },
          "reportFormat": {
            "$ref": "#/components/schemas/ReportFormatEnum"
          },
          "reportType": {
            "$ref": "#/components/schemas/ReportTypeEnum"
          },
          "withTS": {
            "type": "boolean"
          },
          "validationType": {
            "$ref": "#/components/schemas/DocumentVerificationTypeEnum"
          }
        },
        "additionalProperties": false
      },
      "RequestComponentModelDto": {
        "required": [
          "documentType",
          "hashalgorithm"
        ],
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "nullable": true
          },
          "hash": {
            "type": "string",
            "nullable": true
          },
          "hashalgorithm": {
            "$ref": "#/components/schemas/HashAlgorithmEnum"
          },
          "fileName": {
            "type": "string",
            "nullable": true
          },
          "mimeType": {
            "type": "string",
            "nullable": true
          },
          "documentType": {
            "$ref": "#/components/schemas/DocumentTypeEnum"
          }
        },
        "additionalProperties": false
      },
      "RequestDocumentDto": {
        "type": "object",
        "properties": {
          "authentication": {
            "$ref": "#/components/schemas/RequesAuthenticationModelDto"
          },
          "document": {
            "$ref": "#/components/schemas/RequestDocumentModelDto"
          }
        },
        "additionalProperties": false
      },
      "RequestDocumentModelDto": {
        "required": [
          "components",
          "documentClassId",
          "name",
          "sourceSystem",
          "storageId"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "storageId": {
            "type": "string",
            "format": "uuid"
          },
          "documentClassId": {
            "type": "string",
            "format": "uuid"
          },
          "sourceSystem": {
            "minLength": 1,
            "type": "string"
          },
          "components": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/RequestComponentModelDto"
            }
          }
        },
        "additionalProperties": false
      },
      "RequestExternalReport": {
        "type": "object",
        "properties": {
          "authentication": {
            "$ref": "#/components/schemas/RequesAuthenticationModelDto"
          },
          "document": {
            "$ref": "#/components/schemas/RequestExternalReportModelDto"
          }
        },
        "additionalProperties": false
      },
      "RequestExternalReportModelDto": {
        "type": "object",
        "properties": {
          "documentContent": {
            "type": "string",
            "nullable": true
          },
          "fileName": {
            "type": "string",
            "nullable": true
          },
          "isSignatureWithTimestamp": {
            "type": "boolean"
          },
          "sourceSystem": {
            "type": "string",
            "nullable": true
          },
          "certificateId": {
            "type": "string",
            "format": "uuid"
          },
          "cryptedPassword": {
            "type": "string",
            "nullable": true
          },
          "storageId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "RequestSignDocument": {
        "required": [
          "authentication",
          "document"
        ],
        "type": "object",
        "properties": {
          "authentication": {
            "$ref": "#/components/schemas/RequesAuthenticationModelDto"
          },
          "document": {
            "$ref": "#/components/schemas/RequestSignDocumentModelDto"
          }
        },
        "additionalProperties": false
      },
      "RequestSignDocumentModelDto": {
        "required": [
          "certificateId",
          "cryptedPassword",
          "documentContent",
          "documentType",
          "fileName",
          "isSignatureWithTimestamp",
          "sourceSystem",
          "storageId"
        ],
        "type": "object",
        "properties": {
          "documentContent": {
            "minLength": 1,
            "type": "string"
          },
          "documentType": {
            "$ref": "#/components/schemas/DocumentTypeEnum"
          },
          "fileName": {
            "minLength": 1,
            "type": "string"
          },
          "isSignatureWithTimestamp": {
            "type": "boolean"
          },
          "sourceSystem": {
            "minLength": 1,
            "type": "string"
          },
          "certificateId": {
            "type": "string",
            "format": "uuid"
          },
          "cryptedPassword": {
            "minLength": 1,
            "type": "string"
          },
          "storageId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "ResponseSignDocument": {
        "required": [
          "document",
          "success",
          "message",
          "errorCode"
        ],
        "type": "object",
        "properties": {
          "document": {
            "$ref": "#/components/schemas/ResponseSignDocumentModelDto"
          },
          "success": {
            "type": "boolean"
          },
          "message": {
            "minLength": 1,
            "type": "string"
          },
          "errorCode": {
            "minLength": 0,
            "type": "string"
